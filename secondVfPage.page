<apex:page standardController="Student__c" sidebar="false">
    <apex:pageBlock title="Student View Page">
        <apex:pageBlockSection collapsible="false" columns="2" title="Student Basic Info">
            <apex:outputField value="{!Student__c.Student_Name__c}"/>
            <apex:outputField value="{!Student__c.Student_Email__c}"/>
            <apex:outputField value="{!Student__c.Contact_Number__c}"/>
        </apex:pageBlockSection>
        
        <apex:pageBlockSection columns="1" title="Student Other Info">
            <apex:outputField value="{!Student__c.Date_of_Birth__c}"/>
            <apex:outputField value="{!Student__c.Initial_Fees__c}"/>
  
        </apex:pageBlockSection>
    </apex:pageBlock>
</apex:page>